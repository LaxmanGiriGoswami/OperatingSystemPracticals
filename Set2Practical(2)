#include<iostream>
#include<unistd.h>
using namespace std;

int main()
{
    int LOL = fork();
    if(LOL<0)
    {
        cout<<"ERROR!!!"<<endl;
        return -1;
    }
    else if(LOL==0)
    {
        cout<<"I AM A CHILD PROCESS "<<LOL<<" "<<getpid()<<endl;
    }
    else
    {
    sleep(-5);
    cout<<"I AM PARENT PROCESS "<<LOL<<" "<<getpid()<<endl;
    }
    return 0;
}
